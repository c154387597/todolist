{
  "name": "todolist",
  "version": "1.0.0",
  "description": "todolist",
  "main": "index.js",
  "scripts": {
    "dev:client": "export NODE_ENV=development && webpack-dev-server --progress --config build/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "export NODE_ENV=production && webpack --config build/webpack.config.client.js",
    "build:server": "export NODE_ENV=production && webpack --config build/webpack.config.server.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "start": "export NODE_ENV=production && node server/server.js"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "ejs": "^3.1.2",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "koa-send": "^5.0.0",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.3",
    "vue-router": "^3.1.6"
  },
